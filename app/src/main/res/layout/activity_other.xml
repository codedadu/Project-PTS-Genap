<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".intentexplicit.OtherActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp">

        <android.support.v7.widget.CardView
            android:id="@+id/cv_1"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            card_view:cardCornerRadius="3dp"
            card_view:cardElevation="2dp"
            card_view:cardUseCompatPadding="true"
            xmlns:tools="http://schemas.android.com/tools">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:paddingTop="10dp"
                    android:paddingRight="10dp"
                    android:paddingLeft="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Get Put Extra"
                    android:textSize="10pt"/>

                <TextView
                    android:paddingLeft="11dp"
                    android:paddingRight="11dp"
                    android:paddingTop="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Load the single data and then is:"
                    android:textSize="6pt"/>

                <TextView
                    android:id="@+id/tv_getdata_single"
                    android:paddingLeft="11dp"
                    android:paddingRight="11dp"
                    android:paddingBottom="15dp"
                    android:paddingTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Text Loading to get..."
                    android:textSize="6pt"/>

            </LinearLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/cv_2"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            card_view:cardCornerRadius="3dp"
            card_view:cardElevation="2dp"
            card_view:cardUseCompatPadding="true"
            xmlns:tools="http://schemas.android.com/tools">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:paddingTop="10dp"
                    android:paddingRight="10dp"
                    android:paddingLeft="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Get Put Extra's"
                    android:textSize="10pt"/>

                <TextView
                    android:paddingLeft="11dp"
                    android:paddingRight="11dp"
                    android:paddingTop="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Load the single data and then is:"
                    android:textSize="6pt"/>

                <TextView
                    android:id="@+id/tv_getdata_1"
                    android:paddingLeft="11dp"
                    android:paddingRight="11dp"
                    android:paddingTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Text 1 Loading to get..."
                    android:textSize="6pt"/>

                <TextView
                    android:id="@+id/tv_getdata_2"
                    android:paddingLeft="11dp"
                    android:paddingRight="11dp"
                    android:paddingTop="6dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Text 2 Loading to get..."
                    android:textSize="6pt"/>

                <TextView
                    android:id="@+id/tv_getdata_3"
                    android:paddingLeft="11dp"
                    android:paddingRight="11dp"
                    android:paddingTop="6dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Text 3 Loading to get..."
                    android:textSize="6pt"/>

                <TextView
                    android:id="@+id/tv_getdata_4"
                    android:paddingLeft="11dp"
                    android:paddingRight="11dp"
                    android:paddingBottom="15dp"
                    android:paddingTop="6dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Text 4 Loading to get..."
                    android:textSize="6pt"/>

            </LinearLayout>

        </android.support.v7.widget.CardView>

    </LinearLayout>

    <Button
        android:onClick="backtoList"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="?android:attr/borderlessButtonStyle"
        android:text="Back to List"/>

</RelativeLayout>